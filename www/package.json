  "keywords": [
    "gatsby"
  ],
  "license": "UNLICENSED",
  "scripts": {
    "build": "gatsby build",
    "build-storybook": "build-storybook",
    "develop": "gatsby develop",
    "format": "npm run format:code && npm run format:style",
    "format:code": "eslint --fix --ignore-path ./.gitignore --ignore-path ./.prettierignore --ext .js,.jsx .",
    "format:style": "stylelint './src/**/*.{css,scss}' --fix",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:code": "eslint --ignore-path .gitignore --ignore-path .prettierignore --ext .js,.jsx .",
    "lint:style": "stylelint './src/**/*.{js,jsx,.css,.scss}'",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "storybook": "start-storybook -s ./static -p 6006",
    "test": "jest --watch",
    "test:pa11y": "pa11y-ci --sitemap http://localhost:9000/sitemap.xml --sitemap-find https://www.example.com/ --sitemap-replace http://localhost:9000"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run format:code"
    ],
    "*.{css,scss}": [
      "npm run format:style"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setup-test-env.js"
  }
}
